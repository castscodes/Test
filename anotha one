#include <iostream>
#include <vector>
using namespace std;

int main() 
{
	vector<int> numbers;
	int number;
	cout<<"Enter some numbers: ";
	//cin>>number;
		while(cin>>number) 
		{
			numbers.push_back(number);
		}	
		for(int i=0;i<numbers.size();i++)
		{
			cout<<numbers[i]<<" ";
		}
	return 0;
}

